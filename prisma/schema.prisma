generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usuario {
  cveUsuario    Int      @id @default(autoincrement())
  nombre        String
  apellidos     String
  correo        String   @unique(map: "Usuario_correo_key")
  username      String   @unique(map: "Usuario_username_key")
  password      String
  fechaRegistro DateTime @default(now())
}
